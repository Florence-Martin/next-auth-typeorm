{
  "name": "next-auth-typeorm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "seed": "ts-node ./seeder/user-seeder.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migration/InitialMigration -d ./data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d ./data-source.ts",
    "migrate": "ts-node ./run-migrations.ts",
    "build:migrations": "tsc -p tsconfig.migrations.json"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "jose": "^5.6.3",
    "next": "^14.2.5",
    "next-auth": "^4.24.7",
    "oauth": "^0.10.0",
    "pg": "^8.12.0",
    "preact-render-to-string": "^6.5.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "util-deprecate": "^1.0.2",
    "winston": "^3.14.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.12.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "node-mocks-http": "^1.15.1",
    "postcss": "^8",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
